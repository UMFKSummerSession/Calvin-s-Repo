    <script src="https://unpkg.com/@pusher/chatkit/dist/web/chatkit.js"></script>
    <script>
    const tokenProvider = new Chatkit.TokenProvider({
  url: "https://us1.pusherplatform.io/services/chatkit_token_provider/v1/1043d16e-bf5c-4965-a575-d4a2840e2655/token"
});

const chatManager = new Chatkit.ChatManager({
  instanceLocator: "v1:us1:1043d16e-bf5c-4965-a575-d4a2840e2655",
  userId: "Admin",
  tokenProvider: tokenProvider
});
    
 chatManager
  .connect()
  .then(currentUser => {
    currentUser.subscribeToRoom({
        roomId: currentUser.rooms[0].id,
        hooks: {
          onNewMessage: message => {
            console.log(`Received new message: ${message.text}`)
          }
        }
      });
    })
  .catch(error => {
    console.error("error:", error);
  });
      
      
      
      
    
    
    </script>
